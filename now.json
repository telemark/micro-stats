{
  "version": 2,
  "name": "micro-stats",
  "alias": [
    "stats.service.t-fk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "DB": "@azure-cosmos-felles-connection",
    "COLLECTION": "stats",
    "JWT_SECRET": "@tfk-jwt-secret"
  },
  "builds": [
    { "src": "/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
{
  "version": 2,
  "name": "micro-stats",
  "alias": [
    "stats.service.t-fk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "DB": "@azure-cosmos-felles-connection",
    "COLLECTION": "stats"
  },
  "builds": [
    { "src": "/index.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "dotenv": "production.env",
  "features": {
    "cloud": "v2"
  }
}
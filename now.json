{
  "version": 2,
  "name": "micro-stats",
  "scope": "telemark",
  "alias": [
    "stats.service.t-fk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@azure-cosmos-felles-connection",
    "MONGODB_COLLECTION": "stats",
    "MONGODB_NAME": "felles",
    "JWT_SECRET": "@tfk-jwt-secret"
  },
  "builds": [
    {
      "src": "/*.md",
      "use": "@now/md"
    },
    {
      "src": "/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/README.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
{
  "version": 2,
  "name": "minelev-robot-stats",
  "scope": "vtfk",
  "alias": [
    "robot-stats.minelev.vtfk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@minelev-prod-db-connection",
    "MONGODB_COLLECTION": "@minelev-prod-db-collection-robot-stats",
    "MONGODB_NAME": "@minelev-prod-db-name",
    "JWT_SECRET": "@minelev-prod-robot-stats-jwt"
  },
  "builds": [
    {
      "src": "/*.md",
      "use": "@now/md"
    },
    {
      "src": "/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/README.html"
    },
    {
      "src": "/stats/(.*)",
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
        "Access-Control-Allow-Methods": "GET, OPTIONS, OPTIONS",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Max-Age": "3600",
        "Content-Type": "application/json; charset=utf-8"
      }
    }
  ]
}